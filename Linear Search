#include <stdio.h>
#include <stdlib.h>
int main()
{	
	int data[5] = { 0 }, no = 0;
	int j, flag=0, location=0;
	for (j = 0; j < 5; j++)
	{
		printf("Enter no %d Values [max 5]: ", j+1);
		scanf_s("%d", &data[j]);
	}
	printf("Enter no to Search: ");
	scanf_s("%d", &no);

	for (j = 0; j < 5; j++)
	{
		if (data[j] == no)
		{
			flag = 1;
			location = j;
			break;
		}
	}
	if (flag == 1)
	{
		printf("Item Found at index %d\n", location + 1);
	}
	else
	{
		printf("Item not Found !\n");
	}
	system("pause");
	return 0;
}
